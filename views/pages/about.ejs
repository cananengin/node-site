<%- contentFor('hero') %>

<section class="hero">
    <div class="hero-content">
        <h2>All About Us!</h2>
        <p>We are the Best Team.</p>
        <a href="/" class="btn btn-lg btn-primary">Drop us a line</a>
    </div>
</section>
<%- contentFor('body') %>
<div class="jumbotron text-center">
    <h1>The Team</h1>
    <div class="row">
        <% for (user of users) { %>
            <div class="col-sm-3">
                <h2><%= user.name %>
                <img src="<%= user.avatar %>" class="img-responsive">
            </div>
        <% } %>
    </div>
</div>